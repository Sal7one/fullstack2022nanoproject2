{
  "name": "storefront_backend",
  "version": "1.0.0",
  "description": "udacity storefront_backend by Sal7one",
  "main": "src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "watch": "tsc-watch --esModuleInterop src/app.ts --outDir ./dist --onSuccess \"node ./dist/src/app.js\"",
    "build": "npx tsc",
    "lint": "eslint --ignore-path .eslintignore --ext .ts .",
    "prettier": "prettier --config .prettierrc src/*.ts --write",
    "jasmine": "jasmine --config=jasmine.json",
    "test": "set ENV=test&& npx tsc && db-migrate db:create test && db-migrate --env test up && jasmine --random=false && db-migrate db:drop test",
    "migrate:up": "npx db-migrate up",
    "migrate:down": "npx db-migrate reset"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "supertest": "^6.3.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.8.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.6.2"
  }
}
